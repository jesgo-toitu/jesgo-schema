{
    "$schema": "../jesgo.json",
    "$id": "/schema/record/vaccination",
    "jesgo:version": "1.0",
    "type": "object",
    "title": "HPVワクチン接種歴",
    "jesgo:unique": true,
    "properties": {
        "接種回数": {
            "type": "string",
            "enum": [
                "なし",
                "1回",
                "2回",
                "3回"
            ]

        },
        "接種開始年齢": {
            "type": "integer",
            "minimum": 0
        },
        "種類": {
            "type": "string",
            "enum": [
                "2価ワクチン",
                "4価ワクチン",
                "9価ワクチン",
                "不明"
            ]
        }
    }
}
