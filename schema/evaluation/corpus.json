{
    "$schema": "../jesgo.json",
    "$id": "/schema/evaluations/corpus",
    "jesgo:version": "1.0",
    "jesgo:parentschema": [
        "/schema/surveillance"
    ],
    "jesgo:unique": true,
    "type": "object",
    "title": "子宮内膜検査",
    "properties": {
        "検査実施日": {
            "type": "string",
            "format": "date",
            "jesgo:set": "eventdate"
        },
        "細胞診": {
            "type": "string",
            "enum": [
                "未実施",
                "陰性",
                "擬陽性",
                "陽性",
                "Negative for malignancy",
                "ATEC-US",
                "AETC-AE",
                "Endometrial hyperplasia",
                "AEH/EIN",
                "Malignant tumor"
            ]
        },
        "生検組織診": {
            "type": "string",
            "jesgo:ui:textarea": true
        }
    }
}
